	wood

	diffusemap	models/darkmod/props/textures/crate_shipping_long_01a_d
	bumpmap		models/darkmod/props/textures/crate_shipping_long_01a_local
    specularmap models/darkmod/props/textures/crate_shipping_long_01a_s

    {
        if ( parm11 > 0 )
        blend       gl_dst_color, gl_one
        map         _white
        rgb         0.40 * parm11
    }
    {
        if ( parm11 > 0 )
        blend       add
        map         models/darkmod/props/textures/crate_shipping_long_01a_d
        rgb         0.15 * parm11
    }


}

